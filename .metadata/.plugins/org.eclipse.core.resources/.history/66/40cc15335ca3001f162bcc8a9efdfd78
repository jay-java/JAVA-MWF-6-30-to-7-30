package core;
class Call{
	public int add(int a,int b) {
		return a+b;
	}
//	public int add(int a,String b) {
//		return a+b;
//	}
	public int add(int a,int b,int c) {
		return a+b+c;
	}
	public int add(int a,int b,int c,int d) {
		return a+b+c+d;
	}
}
public class P012_polymorphism {
	public static void main(String[] args) {
		Call c = new Call();
		c.add(1, 2, 3);
	}
}
