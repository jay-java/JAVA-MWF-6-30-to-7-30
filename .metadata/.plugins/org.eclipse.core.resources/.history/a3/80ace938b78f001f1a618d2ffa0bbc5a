package core;
//primitive -> byte,short,int,long,float,double,char,boolean
//non-primitive -> array,string,class
public class P002_DataType {
	public static void main(String[] args) {
		byte b = 12; //1
		System.out.println(b);
		short s= 12; //2
		System.out.println(s);
		int i = 12; //4
		long l = 12; //8
		float f = 3.14f; //4
		double d = 3.13; //8
		char c=  '#';
		boolean bool = false;
		
	}
}
