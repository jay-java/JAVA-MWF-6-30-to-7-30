package core;

public class P009_String {
	public static void main(String[] args) {
		char c[] = { 'J', 'A', 'V', 'A' };
		System.out.println(c);
		String c1 = "java31245#$^%^^{}:";
		System.out.println(c1);
		String s1 = "java";
		String s2 = "tech";
		System.out.println(s1.concat(s2));
		System.out.println(s1);
		
	}
}
