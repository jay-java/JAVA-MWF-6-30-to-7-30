package core;

import java.util.HashMap;
import java.util.Map;

public class P028_Map {
	public static void main(String[] args) {
		Map map = new HashMap();
		map.put(1, "java");
		map.put(34.3, "python");
		map.put(false, true);
		map.put(122, 'g');
		map.put(null, null);
		System.out.println(map);
		
	}
}
