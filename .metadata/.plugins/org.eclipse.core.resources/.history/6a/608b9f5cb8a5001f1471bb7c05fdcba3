package core;
class Student{
	int id;
	String name;
	int std;
	
	public Student() {
		
	}
	public Student(int id,String name,int std) {
		this.id = id;
		this.name = name;
		this.std  =std;
	}
	public void setId(int id) {
		this.id = id;
	}
}
public class P015_Encapsulation {
	public static void main(String[] args) {
		Student s1 = new Student();
		s1.id = 1;
		s1.name="java";
		s1.std=4;
		
	}
}
